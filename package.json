  "scripts": {
    "dev:tailwind": "tailwindcss -i src/styles/tailwind.css -o src/styles/index.css -w",
    "dev:react": "react-scripts start",
    "start": "npm-run-all --parallel dev:*",
    "tailwind:build": "NODE_ENV=production tailwindcss -i src/styles/tailwind.css -o src/styles/index.css --minify",
    "prebuild": " npm run tailwind:build",
    "build": "react-scripts build",
    "inline": "npm run build && gulp"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "tailwindui",
    "create-react-app"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.3.8",
    "@emotion/react": "^11.4.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "aws-amplify": "^4.3.0",
    "axios": "^0.21.1",
    "notyf": "^3.10.0",
    "react": "^18.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.0.0",
    "react-dropzone": "^11.3.4",
    "react-paginate": "^8.1.0",
    "react-pdf": "^5.5.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.0.24",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "gulp": "^4.0.2",
    "gulp-inline-source": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12"
  }
